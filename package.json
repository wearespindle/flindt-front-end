{
  "name": "Flindt",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "2.1.3"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "moment": "^2.19.3",
    "node-sass": "4.11.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-notification-system-redux": "^1.2.0",
    "react-paginate": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-time": "^4.3.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-promise": "^0.5.3"
  },
  "scripts": {
    "start": "npx npm-run-all -p watch-css start-js",
    "build": "npx npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "npx node-sass src/ -o src/",
    "watch-css": "npm run build-css && npx node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
